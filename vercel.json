{
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*).(js|css|png|jpg|jpeg|gif|ico|json|map|txt|xml|svg)$",
        "dest": "/$1.$2",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
    ]
  }